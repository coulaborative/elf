<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.fenlisproject.elf.sample">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="ANDROID.PERMISSION.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name="com.fenlisproject.elf.sample.SampleApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.fenlisproject.elf.sample.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.fenlisproject.elf.sample.ViewBindingActivity"
            android:label="@string/title_activity_view_binding"></activity>
        <activity
            android:name="com.fenlisproject.elf.sample.SessionActivity"
            android:label="Session Demo"></activity>
        <activity
            android:name="com.fenlisproject.elf.sample.FormValidationActivity"
            android:label="Session Demo"></activity>
    </application>

</manifest>
